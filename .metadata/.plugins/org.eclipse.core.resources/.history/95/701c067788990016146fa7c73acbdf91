print(list(range(1, 6)))
print(list(range(0, 11, 2))) #등차수열
print(list(range(-10,-100, -20)))
print(set(range(1, 6)))
print(tuple(range(1, 6)))
print(list(range(10))) #초기치 안 줄 경우 0 부터 시작

print()
for i in range(6):
    print(i, end= ' ')
    
print()

# 1 ~ 10 까지의 숫자의 합?
for i in range(1, 11):
    sum(i);
    print(i)

# 2 - 9 구구단 출력

